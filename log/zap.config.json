{
  "level": "debug",
  "named": {
    "log1": "debug",
    "log2": "info",
    "log3": "warn",
    "log4": "error"
  },
  "encoding": "console",
  "outputPaths": [
    "stdout"
  ],
  "errorOutputPaths": [
    "stderr"
  ],
  "encoderConfig": {
    "messageKey": "msg",
    "levelKey": "level",
    "levelEncoder": "lowercase",
    "timeEncoder": "ISO8601"
  }
}